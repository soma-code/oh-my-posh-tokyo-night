  "version": 4,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "foreground": "#c0caf5",
          "background": "#7e06f2",
          "leading_diamond": " ",
          "template": " {{ if .WSL }}WSL at {{ end }}{{ .Icon }} ",
          "options": {
            "linux": "\uF31b"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#7f62f7",
          "powerline_symbol": " ",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#7499fa",
          "powerline_symbol": " ",
          "template": " {{ .Path }} ",
          "options": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#3f7fe8",
          "powerline_symbol": " ",
          "template": "  {{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} "
        },
        {
          "type": "docker",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#6055e9",
          "powerline_symbol": " ",
          "template": "  {{ .Context }} "
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#5d00f7",
          "powerline_symbol": " ",
          "template": "  {{ .Full }} "
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#3a4cff",
          "powerline_symbol": " ",
          "template": "  {{ .Full }} "
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "#1a1b26",
          "background": "#01fcf9",
          "powerline_symbol": " ",
          "template": "  {{ .Full }} "
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#1a1b26",
          "background": "#51cbfd",
          "trailing_diamond": " ",
          "template": " ᚐᚈᚂᚐᚄ {{ .CurrentDate | date .Format }} ",
          "options": {
            "time_format": "15:04"
          }
        },
        {
          "type": "status",
          "style": "diamond",
          "foreground": "#1a1b26",
          "background": "#db4b4b",
          "trailing_diamond": " ",
          "leading_diamond": "",
          "template": "   {{ .Code }} {{ if .Reason }}({{ .Reason }}){{ end }} ",
          "options": {
            "always_enabled": false
          }
        }
      ]
    }
  ]
}
